{
  "name": "Fitness Tracker - Complete Health & Nutrition",
  "short_name": "FitnessTracker",
  "description": "Track your nutrition, workouts, sleep, and wellness with barcode scanning, meal planning, and comprehensive health monitoring",
  "version": "1.0.0",
  "lang": "en-US",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#667eea",
  "background_color": "#ffffff",
  "categories": ["health", "fitness", "lifestyle", "productivity"],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard with nutrition tracking and progress charts"
    },
    {
      "src": "/screenshots/mobile-nutrition.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Nutrition tracking with barcode scanning"
    },
    {
      "src": "/screenshots/mobile-barcode.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Barcode scanner for food products"
    },
    {
      "src": "/screenshots/mobile-workouts.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Workout tracking and exercise logging"
    }
  ],
  "shortcuts": [
    {
      "name": "Log Meal",
      "short_name": "Meal",
      "description": "Quickly log a meal or snack",
      "url": "/#/nutrition?action=add",
      "icons": [
        {
          "src": "/icons/shortcut-meal.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Scan Barcode",
      "short_name": "Scan",
      "description": "Scan product barcode",
      "url": "/#/nutrition?action=scan",
      "icons": [
        {
          "src": "/icons/shortcut-scan.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Log Workout",
      "short_name": "Workout",
      "description": "Start or log a workout",
      "url": "/#/activity?action=add",
      "icons": [
        {
          "src": "/icons/shortcut-workout.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Track Sleep",
      "short_name": "Sleep",
      "description": "Log sleep data",
      "url": "/#/wellness?action=sleep",
      "icons": [
        {
          "src": "/icons/shortcut-sleep.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+fitness",
      "url": "/#/import?data=%s"
    }
  ],
  "share_target": {
    "action": "/#/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "photos",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/#/import",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "image/*": [".jpg", ".jpeg", ".png", ".webp"]
      }
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.fitnesstrack.app",
      "id": "com.fitnesstrack.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/fitness-tracker/id123456789"
    }
  ]
}